# Service config

JWT_EXPIRATION_DAYS=90
API_KEY_EXPIRATION_DAYS=700
SEED_PROJECT_NAME=AppifyHub
SEED_OWNER_NAME=Owner
SEED_OWNER_SECRET=12345678!!!!!!!!abcdefgh
