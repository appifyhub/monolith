# IMPORTANT: Keep environment variables in sync with docker configurations

# DB Config

DB_TYPE=postgresql
POSTGRES_HOST=db
POSTGRES_DB=appifyhub
POSTGRES_USER=appifyhub
POSTGRES_PASS=appifyhub
POSTGRES_HOST_AUTH_METHOD=trust

# Signing config

KEYSTORE_FILE=keystore/debug.jks
KEYSTORE_PASS=debug!
KEYSTORE_KEY=debug
